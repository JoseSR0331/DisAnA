def identCuadrilatero(lado1, lado2, lado3, lado4):
    if lado1 == lado2 == lado3 == lado4:
        return "un cuadrado"
    elif lado1 == lado3 and lado2 == lado4:
        return "un rectángulo"
    else:
        return "otro cuadrilátero"

#Ingreso de medidas
lado1 = float(input("Ingrese el primer lado: "))
lado2 = float(input("Ingrese el segundo lado: "))
lado3 = float(input("Ingrese el tercer lado: "))
lado4 = float(input("Ingrese el cuarto lado: "))

# Identificar el tipo de cuadrilátero
tipoCuadrilatero = identCuadrilatero(lado1, lado2, lado3, lado4)

# Mostrar resultado
print("El cuadrilatero ingresado es: ", tipoCuadrilatero)
